{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/((?!assets|favicon\\.ico|robots\\.txt|manifest\\.webmanifest|sw\\.js|pwa-.*\\.png|apple-touch-icon\\.png|masked-icon\\.svg|placeholder\\.svg).*)",
      "dest": "/index.html"
    }
  ]
}